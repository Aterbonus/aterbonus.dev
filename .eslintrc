{
	"extends": ["eslint:recommended", "@nuxtjs/eslint-config-typescript", "plugin:vue/vue3-recommended", "plugin:prettier/recommended"],
	"rules": {
		"eqeqeq": "error",
		"no-console": "warn",
		"no-tabs": "off",
		"indent": "off",
		"prettier/prettier": "error",
		"vue/multi-word-component-names": "off"
	},
	"ignorePatterns": ["node_modules", "build", "dist", "public"]
}
